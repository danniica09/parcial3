/**
 *
 * @author 102
 */
public class rutas {
    

    String  consultarEstacion;
   
    String consultaRuta( String ruta)
    {
       
        if (ruta.startsWith("1"))
               
                {
                   consultarEstacion= "el salitre";
                   
                }
        else {
            consultarEstacion= "Soacha";
        }

        return consultarEstacion;
    }
   
}

